<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="EnhancedEcommerce">
    </transfer>

    <transfer name="EcImpressions">
        <property name="currencyCode" type="string"/>
        <property name="impressions" type="EnhancedEcommerceProduct[]"/>
    </transfer>

    <transfer name="EnhancedEcommerceProduct">
        <property name="id" type="int"/>
        <property name="name" type="string"/>
        <property name="variant" type="string"/>
        <property name="price" type="float"/>
        <property name="list" type="string"/>
        <property name="position" type="int"/>
    </transfer>
</transfers>
